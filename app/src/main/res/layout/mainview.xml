    android:layout_height="match_parent"
    android:id="@+id/tuckRelativeLayout"
    android:background="@drawable/gdo"
    >

    <Button
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:textSize="20sp"
        android:textStyle="bold"
